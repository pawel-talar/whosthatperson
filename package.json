{
  "name": "whosthatperson",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "dev:workers": "npm run build && npx wrangler dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test --config playwright.config.mjs",
    "e2e:server": "npm run d1:apply:local && npx wrangler dev --local --port 4321",
    "d1:apply": "npx wrangler d1 migrations apply whosthatperson --remote",
    "d1:apply:local": "npx wrangler d1 migrations apply whosthatperson --local",
    "postbuild": "node ./scripts/copy-assetsignore.mjs",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "@playwright/test": "^1.49.1",
    "miniflare": "^4.20260128.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "vitest": "^1.6.0",
    "wrangler": "^4.61.1"
  },
  "overrides": {
    "undici": "^7.19.2",
    "miniflare": "^4.20260128.0",
    "wrangler": "^4.61.1"
  }
}
